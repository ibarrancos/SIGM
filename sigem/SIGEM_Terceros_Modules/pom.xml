<?xml version="1.0" encoding="UTF-8" standalone="no"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	
	<modelVersion>4.0.0</modelVersion>
	
	<parent>
		<groupId>es.ieci.tecdoc.sigem</groupId>
		<artifactId>sigem_main</artifactId>
		<version>3.0.1-M3</version>
	</parent>

	<artifactId>SIGEM_Terceros_Modules</artifactId>
	<packaging>pom</packaging>
	<name>SIGEM_Terceros_Modules</name>

	<description>Componentes de Terceros en SIGEM</description>
	
	<profiles>
		<profile>
			<id>generate-config</id>
			<activation>
				<property>
					<name>config</name>
				</property>
			</activation>
			<modules>
				<module>SIGEM_TercerosWS-resources-configurable</module>
			</modules>
		</profile>		
		<profile>
			<id>generate-jars</id>
			<activation>
				<property>
					<name>jars</name>
				</property>
				<activeByDefault>true</activeByDefault>
			</activation>			
			<modules>
				<module>SIGEM_Terceros</module>
				<module>SIGEM_TercerosWSClient</module>
				<module>SIGEM_TercerosWS-resources-configurable</module>
			</modules>
		</profile>
		<profile>
			<id>generate-wars</id>
			<activation>
				<property>
					<name>wars</name>
				</property>
			</activation>
			<modules>
				<module>SIGEM_TercerosWS</module>
			</modules>
		</profile>
	</profiles>

</project>
